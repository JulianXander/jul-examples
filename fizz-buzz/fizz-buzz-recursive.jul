evenlyDivisibleBy = (divisor: NonZeroInteger) =>
	(dividend: Integer) => dividend.modulo(divisor).equal(0)

fizzBuzz = (index: PositiveInteger) =>
	index ?
		1 => ()
		Any => fizzBuzz(index.subtract(1))
	message = index ?
		evenlyDivisibleBy(15) => §FizzBuzz§
		evenlyDivisibleBy(5) => §Buzz§
		evenlyDivisibleBy(3) => §Fizz§
		Any => index
	log(message)

fizzBuzz(100)